<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.yuanyu.app.mapper.sys.SysRoleMapper">
    <select id="selectByRoleName" resultType="cn.yuanyu.app.entity.sys.SysRoleEntity">
        select *
        from sys_role
        where role_name = #{roleName}
    </select>
</mapper>